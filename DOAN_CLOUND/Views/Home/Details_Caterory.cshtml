
@{
    ViewBag.Title = "Details_Caterory";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@model DOAN_CLOUND.Models.Category
<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <div class="form-horizontal" style="width:25%; margin:20px auto; padding:10px; box-shadow:1px 1px 2px 2px #888888; border-radius:10px">
                <h4>Thông Tin Loại Hàng </h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>ID</dt>
                    <dd>@Model.Id</dd>

                    <dt>Tên loại</dt>
                    <dd>@Model.Name</dd>
                    <p>
                        @Html.ActionLink("Back to List", "Index_Category", "", new { @class = "btn btn-info" })
                    </p>
                </dl>
            </div>

        </div>
    </div>
</div>

